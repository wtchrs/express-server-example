<html>
<head>
  <title>Meadowlark Travel</title>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
  <link rel="stylesheet" href="/stellar/css/main.css" />
  <noscript><link rel="stylesheet" href="/stellar/css/noscript.css" /></noscript>
  {{{sections.head}}}
</head>

<body class="is-preload">

  <!-- Wrapper -->
  <div id="wrapper">

    <!-- Header -->
    <header id="header">
      <a href="/">
        <h1>Meadowlark Travel</h1>
        <p>Travel with Meadowlark Travel</p>
      </a>
    </header>

    <!-- Main -->
    <div id="main">

      <!-- Content -->
      <section id="content" class="main">
        {{#if flash}}
          <!-- Flash message -->
          <div class="alert alert-dismissible alert-{{flash.type}}">
            <button class="close" type="button" data-dismiss="alert" aria-hidden="true">&times;</button>
            <strong>{{flash.intro}}</strong>
            {{{flash.message}}}
          </div>
        {{/if}}

        <h1>{{sections.title}}</h1>
        {{{body}}}
      </section>

    </div>

    <!-- Footer -->
    <footer id="footer">
      <section>
        <h2>About</h2>
        <p>Welcome to Meadowlark Travel.</p>
        <ul class="actions">
          <li><a href="/about" class="button" data-test-id='about'>Learn More</a></li>
        </ul>
      </section>
      <section>
        <h2>Contact</h2>
        <dl class="alt">
          <dt>Address</dt>
          <dd>1234 Somewhere Road &bull; Nashville, TN 00000 &bull; USA</dd>
          <dt>Phone</dt>
          <dd>(000) 000-0000 x 0000</dd>
          <dt>Github</dt>
          <dd>
            <a href="https://github.com/wtchrs/express-server-example" target="_blank">wtchrs/express-server-example</a>
          </dd>
          <dt>Email</dt>
          <dd><a href="#">information@untitled.tld</a></dd>
        </dl>
        <ul class="icons" hidden>
          <li><a href="#" class="icon brands fa-twitter alt"><span class="label">Twitter</span></a></li>
          <li><a href="#" class="icon brands fa-facebook-f alt"><span class="label">Facebook</span></a></li>
          <li><a href="#" class="icon brands fa-instagram alt"><span class="label">Instagram</span></a></li>
          <li><a href="https://github.com/wtchrs/express-server-example" class="icon brands fa-github alt"><span class="label">GitHub</span></a></li>
          <li><a href="#" class="icon brands fa-dribbble alt"><span class="label">Dribbble</span></a></li>
        </ul>
      </section>
      <p class="copyright">&copy; 2022 Meadowlark Travel. Design: <a href="https://html5up.net">HTML5 UP</a>.</p>
    </footer>

  </div>

  <!-- Scripts -->
  <script src="/stellar/js/jquery.min.js"></script>
  <script src="/stellar/js/jquery.scrollex.min.js"></script>
  <script src="/stellar/js/jquery.scrolly.min.js"></script>
  <script src="/stellar/js/browser.min.js"></script>
  <script src="/stellar/js/breakpoints.min.js"></script>
  <script src="/stellar/js/util.js"></script>
  <script src="/stellar/js/main.js"></script>
  {{{sections.script}}}

</body>
</html>
